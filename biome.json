{
  "$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
  "extends": ["@radashi-org/biome-config"],
  "linter": {
    "rules": {
      "complexity": {
        "noBannedTypes": "off"
      },
      "style": {
        "noUnusedTemplateLiteral": "off"
      },
      "suspicious": {
        "noFallthroughSwitchClause": "off",
        "noUnsafeDeclarationMerging": "off"
      }
    }
  }
}
